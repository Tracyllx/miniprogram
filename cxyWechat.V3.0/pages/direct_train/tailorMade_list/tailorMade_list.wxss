page {
    width: 100%;
    height: 100%;
    background-color: #eee;
}

.list-box {
    height: 100%;
    width: 94%;
    padding: 0 3%;
}

.list-look-all {
    font-size: 30rpx;
    text-align: center;
    line-height: 60rpx;
    color: #339999;
}

.list-item {
    width: 100%;
    margin-bottom: 4%;
    background: #fff;
    position: relative;
}

.list-item-price {
    position: absolute;
    top: 10%;
    left: 0;
    max-width: 170rpx;
    line-height: 50rpx;
    padding: 0 8rpx;
    font-size: 26rpx;
    color: #fff;
    background: rgba(0, 131, 123, .6);
}

.list-item-mt {
    margin-top: 3%;
}

.list-item-img {
    width: 100%;
    height: 350rpx;
}

.list-item-img>image {
    width: 100%;
    height: 100%;
    vertical-align: middle;
}

.list-item-txt {
    width: 100%;
    padding: 30rpx 0;
}

.orgin-name {
    font-size: 34rpx;
    text-align: center;
}

.product-name {
    font-size: 26rpx;
    text-align: center;
    color: #999;
    line-height: 60rpx;
}

/* 没有更多数据了 */

.nonedata {
    width: 100%;
    text-align: center;
    color: #01827d;
    font-size: 27rpx;
}

.c9 {
    line-height: 70rpx;
    height: 70rpx;
    color: #999;
}