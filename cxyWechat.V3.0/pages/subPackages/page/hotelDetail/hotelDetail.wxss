}

/* 上车点 */

.boarding {
    padding: 14rpx;
    padding-left: 24rpx;
    overflow: hidden;
    background: #fff;
    /* border-bottom: 1rpx solid #c6c6c6; */
}

/* 展示上车点的左右两边view */

.boarding>view {
